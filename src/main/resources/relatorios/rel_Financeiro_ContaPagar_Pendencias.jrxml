	<parameter name="vencimento_inicio" class="java.sql.Date"/>
	<parameter name="vencimento_final" class="java.sql.Date"/>
	<queryString language="SQL">
		<![CDATA[select 
	c.id,
	c.data_emissao,
	c.vencimento,
	c.valor,
	c.status,
	c.id_fornecedor,
	c.recibo,
	c.tem_nota,
	c.id_empresa,
	f.id as cod_fornecedor,
	f.nome,
	f.telefone, 
	e.id as cod_empresa,
	e.sigla
from
	conta_pagar c inner join fornecedor f on c.id_fornecedor = f.id
	inner join empresa e on c.id_empresa = e.id
where
	((c.id_empresa = $P{id_empresa} or $P{id_empresa}  is null) and (c.recibo = false or c.tem_nota = false))
and
	((c.id_fornecedor = $P{id_fornecedor} or $P{id_fornecedor}  is null) and (c.recibo = false or c.tem_nota = false))
and
	((c.status = $P{status} or $P{status}  is null) and (c.recibo = false or c.tem_nota = false))
and
	(((c.data_emissao between $P{emissao_inicio} and $P{emissao_final})) 
		or (($P{emissao_inicio}  is null) or ($P{emissao_final} is null)) 
		and (c.recibo = false or c.tem_nota = false))
and
	(((c.vencimento >= $P{vencimento_inicio} and c.vencimento <= $P{vencimento_final})) 
		or (($P{vencimento_inicio}  is null) or ($P{vencimento_final} is null)) 
		and (c.recibo = false or c.tem_nota = false)	)
order by c.vencimento]]>
	</queryString>
